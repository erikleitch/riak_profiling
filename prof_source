export RIAK_PROF_BASE=`pwd`

if [ ! -d python_scripts/riak_graphviz ]
then
    git clone "ssh://erikleitch@github.com/erikleitch/riak_graphviz.git" -b master python_scripts/riak_graphviz
fi

export PYTHONPATH=${PYTHONPATH}:${RIAK_PROF_BASE}/python_scripts:${RIAK_PROF_BASE}/python_scripts/riak_graphviz

source bash_scripts/generic_bash_fns
source bash_scripts/profiler_bash_fns
source bash_scripts/put_bash_fns

